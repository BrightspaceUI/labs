    "@brightspace-ui/stylelint-config": "^0.8",
    "@brightspace-ui/testing": "^1",
    "@web/dev-server": "^0.3",
    "eslint": "^8",
    "eslint-config-brightspace": "^1",
    "messageformat-validator": "^2",
    "stylelint": "^15"
  },
  "files": [
    "labs.serge.json",
    "/src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@brightspace-ui/core": "^2",
