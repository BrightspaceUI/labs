{
  "name": "@brightspace-ui-labs/ou-filter",
  "version": "1.1.0",
  "description": "Org unit tree filter",
  "repository": "https://github.com/BrightspaceUILabs/ou-filter.git",
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "homepage": "https://github.com/BrightspaceUILabs/ou-filter#readme",
  "bugs": {
    "url": "https://github.com/BrightspaceUILabs/ou-filter/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "locales",
    "ou-filter.js",
    "tree-filter.js",
    "tree-selector-node.js",
    "tree-selector.js"
  ],
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:lit && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer assessment-quality-dashboard.js demo test components",
    "lint:style": "stylelint \"**/*.js\"",
    "start": "web-dev-server --node-resolve --app-index demo/index.html --open --watch",
    "test": "npm run lint && npm run test:headless",
    "test:diff": "mocha ./test/visual-diff/**/*.visual-diff.js -t 40000",
    "test:diff:golden": "mocha ./test/visual-diff/**/*.visual-diff.js -t 40000 --golden",
    "test:diff:golden:commit": "commit-goldens",
    "test:headless": "karma start",
    "test:headless:watch": "karma start --auto-watch=true --single-run=false",
    "test:sauce": "karma start karma.sauce.conf.js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@brightspace-ui/stylelint-config": "0.1.0",
    "@brightspace-ui/visual-diff": "^3.0.0",
    "@open-wc/testing": "^2",
    "@open-wc/testing-karma": "^4.0.1",
    "@polymer/polymer": "^3.4.1",
    "@web/dev-server": "^0.1.17",
    "deepmerge": "^4",
    "es-dev-server": "^1.55.1",
    "eslint": "^7.15.0",
    "eslint-config-brightspace": "^0.13.0",
    "eslint-plugin-html": "^6",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-lit": "~1.4.0",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "eslint-plugin-sort-class-members": "^1",
    "karma-sauce-launcher": "^4",
    "lit-analyzer": "^1.2.0",
    "puppeteer": "^8.0.0",
    "sinon": "^9.0.3",
    "stylelint": "^13.12.0"
  },
  "dependencies": {
    "@adobe/lit-mobx": "0.0.x",
    "@brightspace-ui/core": "^1.136.3",
    "@brightspace-ui/intl": "^3.2.0",
    "array-flat-polyfill": "^1.0.1",
    "lit-element": "^2",
    "lit-html": "^1.3.0",
    "mobx": "^5.15.7"
  }
}
