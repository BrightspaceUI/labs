{
  "name": "@brightspace-ui-labs/grade-result",
  "description": "A web component used for rendering grades in Brightspace",
  "type": "module",
  "repository": "https://github.com/BrightspaceUILabs/grade-result.git",
  "version": "2.1.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer d2l-grade-result.js demo src test",
    "start": "web-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "web-test-runner",
    "test:headless:watch": "web-test-runner --watch"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@open-wc/testing": "^3",
    "@web/dev-server": "^0.1",
    "@web/test-runner": "^0.13",
    "@web/test-runner-saucelabs": "^0.6",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.16",
    "eslint-plugin-html": "^6",
    "eslint-plugin-import": "^2",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "lit-analyzer": "^1",
    "sinon": "^9"
  },
  "dependencies": {
    "@brightspace-ui/core": "^2",
    "d2l-polymer-siren-behaviors": "github:Brightspace/polymer-siren-behaviors#semver:^1",
    "lit-element": "^3",
    "siren-sdk": "github:BrightspaceHypermediaComponents/siren-sdk#semver:^2"
  }
}
